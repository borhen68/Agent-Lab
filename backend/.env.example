# Server
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/agent_lab
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=10

# Redis
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# API
ANTHROPIC_API_KEY=sk-ant-xxxxxxxxxxxx
GEMINI_API_KEY=
OPENAI_API_KEY=sk-proj-xxxxxxxxxxxx
DEFAULT_LLM_PROVIDER=anthropic
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022
GEMINI_MODEL=gemini-2.5-flash
OPENAI_MODEL=gpt-4o-mini
# Optional: Tavily API key for reliable web search (https://tavily.com)
# Optional: Serper.dev API key as secondary fallback (https://serper.dev)
# Set at least one key. Without both, web-search returns a clear configuration error.
TAVILY_API_KEY=
SERPER_API_KEY=

# Logging
LOG_LEVEL=debug

# CORS
CORS_ORIGIN=http://localhost:5173

# Security
SESSION_SECRET=your-secret-key-here-min-32-chars-long

# Features
ENABLE_LEARNING=true
CONFIDENCE_GATE_ENABLED=true
CONFIDENCE_GATE_MIN_TOTAL=26
CONFIDENCE_GATE_MIN_MARGIN=2
CONFIDENCE_GATE_MIN_ACCURACY=6.5
MAX_AGENTS=3
AGENT_TIMEOUT_MS=120000
